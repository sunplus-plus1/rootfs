# latch av1 stc
devmem 0x9c0030e8 32 1 ; devmem 0x9c0030e0 32 ; devmem 0x9c0030e4 32

if [ -d /sys/kernel/debug ];then
	mount -t debugfs none /sys/kernel/debug
fi

if [ 0 -eq 1 ];then
        echo "Ramspeed test"
        time ramspeed -b 1
        time ramspeed -b 2
        time ramspeed -b 3
fi

if [ 0 -eq 1 ];then
        echo "User mem test"
        memspeed_test 10 4 1
fi

if [ 0 -eq 1 ];then
        echo "add lo"
        ifconfig -a lo ; ifconfig lo up
        iperf -s -V &
        cmd="iperf -c ::1 -i 1 -t 5 -V -m -M 1500" && echo $cmd
        $cmd
        cmd="iperf -c ::1 -i 1 -t 5 -V -m" && echo $cmd
        $cmd
fi

if [ 0 -eq 1 ];then
	echo "Stress ..."
	rstress.sh &
	rstress.sh &
	rstress.sh &
	rstress.sh &
	echo 1|top -d $((30*60)) &
fi

echo "extra done"
